        <script>
            $(window).load(function () {
                $('.content').hide();
                $('.click').click(function () {
                    $('.content').slideUp(1)
                    $(this).next('.content').toggle();
                });
                $('.click').on('click', function (event) {
                    var target = $(this);
                    if (target.length) {
                        event.preventDefault();
                        $('html, body').animate({
                            scrollTop: target.offset().top
                        }, 1000);
                    }
                });
            });
        </script>
        <style>
            .click {
                cursor: pointer;
            }
            
            #EmergencyNumbers .content {
                display: block;
            }

